{
    "extends": "tslint:recommended",
    "rules": {
        "member-access": false,
        "no-console": [
            false
        ],
        "object-literal-sort-keys": false,
        "variable-name": [
            true,
            "ban-keywords"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ],
        "no-var-requires": false,
        "no-require-imports": false
    }
}