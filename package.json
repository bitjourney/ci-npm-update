{
  "name": "npm-update-scanner",
  "version": "1.0.0",
  "description": "Scans NPM dependencies to get their version-to-version diffs",
  "main": "index.js",
  "scripts": {
    "setup": "npm install",
    "build": "npm run lint && tsc",
    "lint": "tslint src/**/*.ts",
    "test": "tsc -p test && mocha --opts test/support/default.opts test.build"
  },
  "keywords": [
    "npm",
    "update",
    "github",
    "ci"
  ],
  "author": "FUJI Goro",
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 4.2.0"
  },
  "dependencies": {
    "@types/node": "*",
    "semver": "^5.3.0",
    "@types/semver": "*",
    "request": "^2.73.0",
    "@types/request": "*"
  },
  "devDependencies": {
    "mocha": "^2.5.1",
    "@types/mocha": "*",
    "npm-run-all": "^2.3.0",
    "power-assert": "^1.4.1",
    "@types/power-assert": "*",
    "source-map-support": "^0.4.0",
    "tslint": "^3.13.0",
    "typescript": ">= 2.0.0",
    "typescript-formatter": "^2.3.0"
  }
}
