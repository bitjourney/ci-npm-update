{
  "name": "ci-npm-update",
  "version": "1.0.0",
  "description": "Keep NPM dependencies up-to-date with CI, providing version-to-version diff for each library",
  "main": "index.js",
  "scripts": {
    "setup": "npm install",
    "build": "npm run lint && tsc",
    "lint": "tslint src/**/*.ts",
    "test": "npm run build && mocha --compilers ts:ts-node/register --opts test/support/default.opts test/**/*.test.ts"
  },
  "keywords": [
    "npm",
    "update",
    "github",
    "ci"
  ],
  "author": "FUJI Goro",
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 6.0.0"
  },
  "dependencies": {
    "@types/moment": "^2.11.28",
    "@types/node": "*",
    "@types/node-getopt": "^0.2.28",
    "@types/request": "*",
    "@types/semver": "*",
    "moment": "^2.14.1",
    "node-getopt": "^0.2.3",
    "request": "^2.73.0",
    "semver": "^5.3.0"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/power-assert": "*",
    "espower-typescript": "^5.0.1",
    "mocha": "^2.5.1",
    "npm-run-all": "^2.3.0",
    "power-assert": "^1.4.1",
    "source-map-support": "^0.4.0",
    "to-iso-string": "0.0.2",
    "ts-node": "^1.2.1",
    "tslint": "^3.13.0",
    "typescript": ">= 2.0.0",
    "typescript-formatter": "^2.3.0"
  }
}
